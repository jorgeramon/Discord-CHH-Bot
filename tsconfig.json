{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "incremental": true,
    "paths": {
      "@funa/*": ["./funa/*"],
      "@user/*": ["./user/*"],
      "@shared/*": ["./shared/*"],
      "@discord/*": ["./discord/*"],
      "@miscellaneous/*": ["./miscellaneous/*"],
      "@twitch/*": ["./twitch/*"]
    }
  }
}
